<%= turbo_frame_tag('book-logs') do %>
<section>
  <div id="no-logs">
    <% if book.logs.empty? %>
      No logs yet
    <% end %>
  </div>
  <div id="logs-list">
    <% book.logs.where.not(page: nil).each do |log| %>
      <%= render log unless log.id.nil? %>
    <% end %>
  </div>
</section>
<% end %>
